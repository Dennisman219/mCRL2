%Example from GL01

sort Frame, D, Bit;
cons frame: D#Bit -> Frame;
     void : Frame;
     d1: D;
     d2: D;
     b0: Bit;
     b1: Bit;
map  new_data: Frame -> D;
     bit: Frame -> Bit;
var  d:D;
     b:Bit; 
eqn  data(frame(d,b)) = d;
     bit(frame(d,b))  = b;
     data(void)       = d1;
     bit(void)        = b0;




init delta;
